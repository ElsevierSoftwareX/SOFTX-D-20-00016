getTeamSide(Away,F,SHF,'Left')        :-
        Away = 0,
        F < SHF,
        !.

getTeamSide(Away,F,SHF,'Right')         :-
        Away = 1,
        F < SHF,
        !.

getTeamSide(Away,F,SHF,'Left')         :-
        Away = 1,
        F > SHF,
        !.

getTeamSide(Away,F,SHF,'Right')         :-
        Away = 0,
        F > SHF,
        !.

