latLngToRegion(X,Y,'Side')              :-
        0=<X,X<110,
        0=<Y,Y<15.85,
        !.

latLngToRegion(X,Y,'Side')              :-
        0=<X,X<110,
        56.15=<Y,Y<72,
        !.

latLngToRegion(X,Y,'insideGoalArea')    :-
        0=<X,X<5.5,
        26.85=<Y,Y<45.15,
        !.

latLngToRegion(X,Y,'insideGoalArea')    :-
        104.5=<X,X<110,
        26.85=<Y,Y<45.15,
        !.

latLngToRegion(X,Y,'insidePenaltyArea') :-
        0=<X,X<16.5,
        15.85=<Y,Y<56.15,
        !.

latLngToRegion(X,Y,'insidePenaltyArea') :-
        93.5=<X,X<110,
        15.85=<Y,Y<56.15,
        !.

latLngToRegion(X,Y,'Center')            :-
        0=<X,X<27.5,
	15.85=<Y,Y<56.15,
        !.

latLngToRegion(X,Y,'Center')            :-
        82.5=<X,X<110,
	15.85=<Y,Y<56.15,
        !.

latLngToRegion(X,Y,'fromOutsideArea')   :-
        15.85=<Y,Y<56.15,
        16.5=<X,X<93.5,
        !.

